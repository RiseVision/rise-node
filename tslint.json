{
  "extends":        [
    "tslint:recommended"

  ],
  "jsRules":        {},
  "rules":          {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}