{
  "id"                  : "Vote",
  "type"                : "object",
  "properties"          : {
    "votes": {
      "type"       : "array",
      "minItems"   : 1,
      "uniqueItems": true,
      "items"      : {
        "type"   : "string",
        "pattern": "^[-+]{1}[0-9a-z]{64}$"
      }
    }
  },
  "required"            : [
    "votes"
  ],
  "additionalProperties": false
}