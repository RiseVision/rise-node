{
  "enableForging": {
    "id": "delegates.enableForging",
    "type": "object",
    "properties": {
      "secret": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "publicKey": {
        "type": "string",
        "format": "publicKey"
      }
    },
    "required": ["secret"]
  },
  "disableForging": {
    "id": "delegates.disableForging",
    "type": "object",
    "properties": {
      "secret": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      }
    },
    "required": ["secret"]
  },
  "forgingStatus": {
    "id": "delegates.forgingStatus",
    "type": "object",
    "properties": {
      "publicKey": {
        "type": "string",
        "format": "publicKey"
      }
    }
  },
  "getDelegate": {
    "id": "delegates.getDelegate",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "format": "username",
        "minLength": 1,
        "maxLength": 20
      },
      "publicKey": {
        "type": "string",
        "format": "publicKey"
      }
    }
  },
  "search": {
    "id": "delegates.search",
    "type": "object",
    "properties": {
      "q": {
        "type": "string",
