    "id": "delegates.getDelegates",
    "type": "object",
    "properties": {
      "orderBy": {
        "type": "string",
        "enum": [
          "vote:desc",
          "vote:asc",
          "votesWeight:desc",
          "votesWeight:asc",
          "address:desc",
          "address:asc",
          "username:desc",
          "username:asc",
          "forgingPK:desc",
          "forgingPK:asc"
        ]
      },
      "limit": {
        "type": "integer",
        "minimum": 1
      },
      "offset": {
        "type": "integer",
        "minimum": 0
      }
    }
