{
  "name": "@risevision/cli",
  "rise_vision": {
    "module": true
  },
  "engine": ">=10",
  "engines": {
    "node": "~10"
  },
  "version": "1.0.0",
  "bin": {
    "rise": "dist/rise"
  },
  "description": "RISE Manager",
  "main": "dist/rise",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npx parcel build src/rise.ts --no-minify -d dist --target node --bundle-node-modules",
    "build:rename": "mv dist/rise.js dist/rise",
    "shebang": "echo \"#!/usr/bin/env node\"|cat - dist/rise > /tmp/out && mv /tmp/out dist/rise",
    "chmod-x": "chmod +x dist/rise",
    "dist": "yarn clean; yarn build; yarn build:rename; yarn shebang; yarn chmod-x"
  },
  "types": "build/main.d.ts",
  "author": "Rise Team, Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>, Shift Team",
  "license": "GPL-3.0",
  "devDependencies": {
    "@carnesen/cli": "^0.3.3",
    "debug": "^4.1.1",
    "parcel": "^1.12.3",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@types/follow-redirects": "^1.5.0",
    "delay": "^4.3.0",
    "dotenv": "^8.0.0",
    "env": "^0.0.2",
    "extend": "=3.0.2",
    "follow-redirects": "^1.7.0",
    "fs-extra": "^8.0.1",
    "mkdirp": "^0.5.1",
    "tree-kill": "^1.2.1"
  }
}
